{
  "name": "frontend-interview",
  "version": "0.0.1",
  "description": "Challenge B2W",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack-dev-server --config ./webpack.config.dev.js --progress --colors --inline --display-error-details",
    "build": "webpack --config ./webpack.config.prod.js --progress --profile --colors --display-error-details",
    "doc": "npm run doc:js && npm run doc:scss",
    "doc:js": "./node_modules/.bin/esdoc -c .esdocrc",
    "doc:scss": "./node_modules/.bin/sassdoc source/assets/scss -c .sassdocrc",
    "lint": "./node_modules/.bin/eslint source/assets/js/**",
    "postinstall": "npm start && npm run doc",
    "start": "./node_modules/.bin/grunt && node server.js",
    "test": "./node_modules/.bin/karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemersonvianna/frontend-interview.git"
  },
  "keywords": [
    "javascript",
    "interview",
    "front-end"
  ],
  "author": "Hemerson Vianna <hemerson.lourenco@gmail.com> (http://hemersonvianna.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hemersonvianna/frontend-interview/issues"
  },
  "homepage": "https://github.com/hemersonvianna/frontend-interview#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "esdoc": "^0.4.7",
    "eslint": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "grunt": "^1.0.1",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-combine-media-queries": "^1.0.20",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-htmlmin": "^1.4.0",
    "grunt-contrib-imagemin": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-karma": "^2.0.0",
    "grunt-sass": "^1.2.0",
    "grunt-webpack": "^1.0.11",
    "html-loader": "^0.4.3",
    "http-proxy-middleware": "^0.17.0",
